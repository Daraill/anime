# Censys API Credentials
CENSYS_URL=https://search.censys.io/api/v2/hosts/search
CENSYS_ID=your_censys_id
CENSYS_SECRET=your_censys_secret

# Server Config
PORT=8080

# CORS Config
# Comma-separated list of allowed origins
# Example: http://example.com,http://anotherdomain.com
ALLOWED_ORIGINS=http://localhost:8080,http://yourdomain.com

# Rate Limiting Config
# Window size in milliseconds (e.g., 15 minutes = 900000 ms)
RATE_LIMIT_WINDOW_MS=900000

# Maximum number of requests per window per IP
RATE_LIMIT_MAX=100
